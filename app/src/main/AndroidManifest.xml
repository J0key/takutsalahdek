<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />


    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ghiblilauncher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Material3.Light.NoActionBar"
        android:roundIcon="@drawable/ghiblilauncher"
        android:supportsRtl="true"
        tools:targetApi="31">
        <activity
            android:name=".DetailActivity"
            android:exported="false" />
        <activity
            android:name=".admin.MainAdminActivity"
            android:exported="false" />
        <activity
            android:name=".admin.EditActivity"
            android:exported="false" />
        <activity
            android:name=".admin.AddActivity"
            android:exported="false" />
        <activity
            android:name=".users.BottomNavbarActivity"
            android:exported="false" />
        <activity
            android:name=".account.TabLayoutActivity"
            android:exported="false" />
        <activity
            android:name=".SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>